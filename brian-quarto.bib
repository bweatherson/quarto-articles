@article{Stanley2008-STAKAC,
	abstract = {This paper is a companion piece to my earlier paper \textquotedblleft{}Fallibilism and Concessive Knowledge Attributions\textquotedblright. There are two intuitive charges against fallibilism. One is that it countenances the truth (and presumably acceptability) of utterances of sentences such as \textquotedblleft{}I know that Bush is a Republican, though it might be that he is not a Republican\textquotedblright. The second is that it countenances the truth (and presumably acceptability) of utterances of sentences such as \textquotedblleft{}I know that Bush is a Republican, even though I\textquoteright{}m not certain that he is\textquotedblright, or \textquotedblleft{}I know that Bush it a Republican, even though it isn\textquoteright{}t certain that he is.\textquotedblright In \textquotedblleft{}Fallibilism and Concessive Knowledge Attributions\textquotedblright, I argue that fallibilism in epistemology does not countenance the truth of utterances of sentences such as \textquotedblleft{}I know that Bush is a Republican, though it might be that he is not a Republican\textquotedblright. In this paper, I argue that there are independent reasons for thinking that utterances of sentences such as \textquotedblleft{}I know that Bush is a Republican, though I\textquoteright{}m not certain that he is\textquotedblright and \textquotedblleft{}I know that Bush is a Republican, though it\textquoteright{}s not certain that he is\textquotedblright are unassertible. More specifically, I argue that these are simply instances of Moore\textquoteright{}s Paradox, such as \textquotedblleft{}Dogs bark, but I don\textquoteright{}t know that they do.\textquotedblright The right account of Moore\textquoteright{}s Paradox does not involve the falsehood of the semantic content of the relevant utterances, but rather their pragmatic unacceptability. So the anti-fallibilist intuitions turn out to have pragmatic, rather than semantic import, and therefore do not tell against the truth of fallibilism. Fallibilism in epistemology is often thought to be theoretically desirable, but intuitively problematic. My purpose with these two papers is to show that fallibilism is not intuitively problematic},
	author = {Jason Stanley},
	date-added = {2018-04-06 17:47:54 +0000},
	date-modified = {2018-04-06 21:33:39 +0000},
	doi = {10.1111/j.1533-6077.2008.00136.x},
	journal = {Philosophical Issues},
	number = {1},
	pages = {35-57},
	title = {{Knowledge and Certainty}},
	volume = {18},
	year = {2008},
	bdsk-url-1 = {https://doi.org/10.1111/j.1533-6077.2008.00136.x}}

