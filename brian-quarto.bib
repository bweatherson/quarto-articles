@article{Weatherson2002-WEAMI,
	abstract = {In \textquotedblleft{}Now the French are invading England\textquotedblright (Analysis 62, 2002, pp. 34-41), Komarine Romdenh-Romluc offers a new theory of the relationship between recorded indexicals and their content. Romdenh-Romluc\textquoteright{}s proposes that Kaplan\textquoteright{}s basic idea, that reference is determined by applying a rule to a context, is correct, but we have to be careful about what the context is, since it is not always the context of utterance. A few well known examples illustrate this. The \textquotedblleft{}here\textquotedblright and \textquotedblleft{}now\textquotedblright in \textquotedblleft{}I am not here now\textquotedblright on an answering machine do not refer to the time and place of the original utterance, but to the time the message is played back, and the place its attached telephone is located. Any occurrence of \textquotedblleft{}today\textquotedblright in a newspaper or magazine refers not to the day the story in which it appears was written, nor to the day the newspaper or magazine was printed, but to the cover date of that publication. Still, it is plausible that for each (token of an) indexical there is a salient context, and that \textquotedblleft{}today\textquotedblright refers to the day of its context, \textquotedblleft{}here\textquotedblright to the place of its context, and soon. Romdenh-Romluc takes this to be true, and then makes a proposal about what the salient context is. It is \textquotedblleft{}the context that Ac would identify on the basis of cues that she would reasonably take U to be exploiting.\textquotedblright (39) Ac is the relevant audience, \textquotedblleft{}the individual who it is reasonable to take the speaker to be addressing\textquotedblright, and who is assumed to be linguistically competent and attentive. (So Ac might not be the person U intends to address. This will not matter for what follows.) The proposal seems to suggest that it is impossible to trick a reasonably attentive hearer about what the referent of a particular indexical is. Since such trickery does seem possible, Romdenh-Romluc\textquoteright{}s theory needs (at least) supplementation. I present two examples of such tricks},
	author = {Brian Weatherson},
	date-added = {2018-04-02 17:41:34 +0000},
	date-modified = {2018-04-02 17:41:34 +0000},
	doi = {10.1093/analys/62.4.308},
	journal = {Analysis},
	number = {4},
	pages = {308-310},
	title = {Misleading Indexicals},
	volume = {62},
	year = {2002},
	bdsk-url-1 = {http://dx.doi.org/10.1093/analys/62.4.308}}

