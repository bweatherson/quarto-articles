@incollection{Nolan2008-NOLMF,
	abstract = {Questions about necessity (or what has to be, or what cannot be otherwise) and possibility (or what can be, or what could be otherwise) are questions about modality. Fictionalism is an approach to theoretical matters in a given area which treats the claims in that area as being in some sense analogous to fictional claims: claims we do not literally accept at face value, but which we nevertheless think serve some useful function. However, despite its name, \textquotedblleft{}Modal Fictionalism\textquotedblright in its usual manifestations is not primarily fictionalism about claims of necessity and possibility, but rather a fictionalist approach to claims about possible worlds. (For instance, modal fictionalism is not normally fictionalist about the claim that \textquotedblleft{}it is possible that there be a species of tail-less kangaroo\textquotedblright, but rather about the claim that \textquotedblleft{}there is a possible world in which there is a species of tail-less kangaroo\textquotedblright.) The practice of taking possible worlds to be merely convenient fictions, or of treating talk about possible worlds as being useful without being literally correct, is quite common in philosophical circles. It is only recently, however, that philosophers have seriously examined the implications of taking possible worlds to be merely fictional objects, like Sherlock Holmes or a frictionless surface},
	author = {Daniel Nolan},
	booktitle = {Stanford Encyclopedia of Philosophy},
	date-added = {2018-04-02 17:41:34 +0000},
	date-modified = {2018-04-02 17:41:35 +0000},
	publisher = {Metaphysics Research Lab, Stanford University},
	title = {{Modal Fictionalism}},
	year = {2008}}

