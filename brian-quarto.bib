@article{Williamson2003-WILE,
	abstract = {On reading the last sentence, did you interpret me as saying falsely that everything --- everything in the entire universe --- was packed into my carry-on baggage? Probably not. In ordinary language, \textquoteleft{}everything\textquoteright and other quantifiers (\textquoteleft{}something\textquoteright, \textquoteleft{}nothing\textquoteright, \textquoteleft{}every dog\textquoteright, ...) often carry a tacit restriction to a domain of contextually relevant objects, such as the things that I need to take with me on my journey. Thus a sentence of the form \textquoteleft{}Everything Fs\textquoteright is true as uttered in a context C if and only if everything that is relevant in C satisfies the predicate \textquoteleft{}F\textquoteright; \textquoteleft{}everything\textquoteright ranges just over the contextually relevant things. Such generality is restricted in a context-relative way},
	author = {Timothy Williamson},
	date-added = {2018-04-02 17:41:34 +0000},
	date-modified = {2018-04-02 17:41:35 +0000},
	doi = {10.1111/j.1520-8583.2003.00017.x},
